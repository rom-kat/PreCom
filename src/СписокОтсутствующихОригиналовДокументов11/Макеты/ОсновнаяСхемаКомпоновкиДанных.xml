			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ДополнительныеОтметки.Документ.Контрагент КАК Поставщик,
	"Входящий счёт № " + ПРЕДСТАВЛЕНИЕ(ДополнительныеОтметки.СчетОтПоставщика.Номер) + " от " КАК СчетОтПоставщикаНомер,
	ДополнительныеОтметки.СчетОтПоставщика.Дата КАК СчетОтПоставщикаДата,
	ДополнительныеОтметки.ПаспортНаПеревозку.Номер КАК ПаспортНаПеревозкуНомер,
	ДополнительныеОтметки.ПаспортНаПеревозку.Дата КАК ПаспортНаПеревозкуДата,
	ВЫБОР
		КОГДА ДополнительныеОтметки.Коносамент
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК Коносамент,
	ВЫБОР
		КОГДА ДополнительныеОтметки.АвиаНакладная
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК АвиаНакладная,
	ВЫБОР
		КОГДА ДополнительныеОтметки.CMR
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК CMR,
	ВЫБОР
		КОГДА ДополнительныеОтметки.ЖДНакладная
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК ЖДНакладная,
	ВЫБОР
		КОГДА ДополнительныеОтметки.ТТН
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК ТТН,
	ВЫБОР
		КОГДА ДополнительныеОтметки.ТН
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК ТН,
	ВЫБОР
		КОГДА ДополнительныеОтметки.ДокументыЗаПростой
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК ДокументыЗаПростой,
	ВЫБОР
		КОГДА ДополнительныеОтметки.ЗаявкаНаПеревозку
			ТОГДА "Отсутствует"
		ИНАЧЕ "Получен"
	КОНЕЦ КАК ЗаявкаНаПеревозку
ПОМЕСТИТЬ ВТ
ИЗ
	РегистрСведений.ДополнительныеОтметки КАК ДополнительныеОтметки
ГДЕ
	(ДополнительныеОтметки.Коносамент
			ИЛИ ДополнительныеОтметки.АвиаНакладная
			ИЛИ ДополнительныеОтметки.CMR
			ИЛИ ДополнительныеОтметки.ЖДНакладная
			ИЛИ ДополнительныеОтметки.ТТН
			ИЛИ ДополнительныеОтметки.ТН
			ИЛИ ДополнительныеОтметки.ДокументыЗаПростой
			ИЛИ ДополнительныеОтметки.ЗаявкаНаПеревозку)
;

