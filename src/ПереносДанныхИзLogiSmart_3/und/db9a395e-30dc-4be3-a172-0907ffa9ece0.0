Процедура НачальноеЗаполнениеКонтрагентовНаСервере()
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		""ВЫБРАТЬ
		|	dbo_Контрагент.ID КАК ID,
		|	dbo_Контрагент.ПолноеИмя КАК Наименование,
